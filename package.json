{
  "name": "@rphk/ui",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "storybook": "start-storybook",
    "build:storybook": "build-storybook",
    "dev:ui": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "dev:storybook": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn dev:ui\" \"yarn storybook\"",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "build-storybook": "build-storybook"
  },
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "@react-spring/web": "^9.6.1",
    "@rphk/constants": "*",
    "@use-gesture/react": "^10.2.23",
    "clsx": "^1.2.1",
    "formik": "^2.2.9",
    "framer-motion": "^8.3.3",
    "next": "^13.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook-dark-mode": "^1.1.2",
    "storybook-tailwind-dark-mode": "^1.0.15",
    "twrnc": "^3.4.1",
    "zod": "^3.20.2",
    "zod-formik-adapter": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-docs": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.14",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "chromatic": "^6.11.4",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-storybook": "^0.6.7",
    "postcss": "^8.4.19",
    "prettier": "^2.8.2",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "serve": "^13.0.2",
    "storybook": "^6.5.14",
    "tailwindcss": "^3.2.4",
    "tsconfig": "*",
    "tsup": "^6.0.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "webpack": "^5.75.0"
  },
  "private": true,
  "_id": "@rphk/ui@0.0.0"
}
