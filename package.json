{
  "name": "@rphk/ui",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "storybook": "sb dev -p 6006",
    "build:storybook": "sb build --docs",
    "dev:ui": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn dev:ui\" \"yarn storybook\"",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "@rphk/constants": "*",
    "clsx": "^1.2.1",
    "eslint-config-custom": "*",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "twrnc": "^3.4.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.0-alpha.56",
    "@storybook/addon-docs": "^7.0.0-alpha.56",
    "@storybook/addon-essentials": "^7.0.0-alpha.56",
    "@storybook/addon-interactions": "^7.0.0-alpha.56",
    "@storybook/addon-links": "^7.0.0-alpha.56",
    "@storybook/builder-vite": "^7.0.0-alpha.56",
    "@storybook/client-api": "^7.0.0-alpha.56",
    "@storybook/preview-web": "^7.0.0-alpha.56",
    "@storybook/react": "^7.0.0-alpha.56",
    "@storybook/react-vite": "^7.0.0-alpha.56",
    "@types/react": "18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.19.0",
    "eslint-plugin-storybook": "^0.6.7",
    "postcss": "^8.4.19",
    "serve": "^13.0.2",
    "storybook": "^7.0.0-alpha.56",
    "tailwindcss": "^3.2.4",
    "tsconfig": "*",
    "tsup": "^6.0.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  },
  "private": true
}
